config_version=5

[application]

config/name="Endless Tower"
run/main_scene="res://scenes/menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameController="*res://scripts/game_controller.gd"
SolanaService="*res://addons/SolanaSDK/Autoload/SolanaService.tscn"
AudioBackground="*res://scenes/BG music/audio_background.tscn"
SilentWolf="*res://addons/silent_wolf/SilentWolf.gd"
global_data="*res://scripts/global_data.gd"
DamageNumbers="*res://scripts/damage_numbers.gd"
TransitionScreen="*res://scenes/transition_screen.tscn"
Global="*res://scripts/singleton/global.gd"
EnemyDamageNumber="*res://scripts/damage_number.gd"
Popups="*res://scenes/popups.tscn"
LoadManager="*res://scripts/load_manager.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://UI/Cursor/Mouse_cursor2.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/silent_wolf/plugin.cfg")

